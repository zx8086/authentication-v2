{
  "$schema": "./node_modules/@stryker-mutator/core/schema/stryker-schema.json",
  "packageManager": "npm",
  "testRunner": "command",
  "commandRunner": {
    "command": "/Users/Simon.Owusu@Tommy.com/WebstormProjects/pvh.services.authentication-v2/scripts/bun-mutation-runner.sh test --reporter=dots ./test/bun"
  },
  "plugins": ["@stryker-mutator/typescript-checker"],
  "checkers": ["typescript"],
  "tsconfigFile": "tsconfig.json",
  "mutate": [
    "src/**/*.ts",
    "!src/**/*.d.ts",
    "!src/types/**/*.ts",
    "!src/server.ts",
    "!src/index.ts",
    "!src/services/legacy/**/*.ts",
    "!src/handlers/profiling.ts",
    "!src/services/profiling.service.ts",
    "!src/utils/logger.ts",
    "!src/utils/performance.ts"
  ],
  "ignorePatterns": ["node_modules", "dist", "scripts", ".github", "docs"],
  "reporters": ["clear-text", "progress"],
  "thresholds": {
    "high": 80,
    "low": 60,
    "break": null
  },
  "incremental": true,
  "incrementalFile": "test/results/mutation/stryker-incremental.json",
  "concurrency": 8,
  "timeoutMS": 30000,
  "timeoutFactor": 1.5,
  "disableTypeChecks": false,
  "cleanTempDir": "always",
  "tempDirName": ".stryker-tmp",
  "coverageAnalysis": "off",
  "mutator": {
    "excludedMutations": [
      "StringLiteral",
      "ObjectLiteral",
      "ArithmeticOperator",
      "BlockStatement",
      "UpdateOperator",
      "OptionalChaining"
    ]
  }
}
